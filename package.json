{
  "name": "vue-ce-ui",
  "version": "1.1.0",
  "private": false,
  "main": "lib/CEUI.common.js",
  "scripts": {
    "serve": "vue-cli-service serve  ",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --dest lib --name CEUI ./packages/index.js",
    "dist": "npm run build && npm run build-bundle",
    "lint": "vue-cli-service lint",
    "pub": "npm run build-bundle && npm publish"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "element-ui": "^2.13.0",
    "vue": "^2.6.10",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential"
    ],
    "rules": {
      "no-console": "off",
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-undef-init": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "## Project setup ``` npm install ```",
  "directories": {
    "example": "examples"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceandeng/vue-ce-ui.git"
  },
  "keywords": [
    "vue",
    "ui"
  ],
  "author": "ocean",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/oceandeng/vue-ce-ui/issues"
  },
  "homepage": "https://github.com/oceandeng/vue-ce-ui#readme"
}